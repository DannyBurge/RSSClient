<resources>
    <!-- Base application theme. -->
    <style name="RSSClient" parent="AppTheme">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/allTextColor</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/material_blue_400</item>
        <!-- Status bar color. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="actionBarTheme">@style/Theme.Design.Light</item>
    </style>
    <!-- Theme Colors. -->
    <color name="bottomBarMenuColor">@color/material_gray_7</color>
    <color name="cardBackground">@color/semi_clear_black</color>
    <color name="cardPicBackground">@color/semi_clear_black</color>
    <color name="allTextColor">@color/myGray50</color>

    <color name="backgroundTempBar">@color/semi_clear_black</color>

    <color name="main_bg_color">@color/white</color>
</resources>