<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rssclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseline_rss_feed_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_rss_feed_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/RSSClient">
        <activity android:name=".NewsReaderActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>